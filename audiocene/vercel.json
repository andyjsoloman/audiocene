{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "/index.html",
      "permanent": false,
      "has": [
        {
          "type": "header",
          "key": "x-vercel-deployment-url",
          "value": "audiocene.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
